package  {		import flash.display.*;	import flash.geom.*;	import flash.events.*;	import flash.net.*;	import flash.text.*;	import fl.transitions.*;	import fl.transitions.easing.*;	import flash.ui.*;		public class XMLLoader extends MovieClip {				private var urlLoader:URLLoader;		internal var main:BenefitCalculator;		public function XMLLoader() {			init();		}				private function init():void {			urlLoader = new URLLoader();		}				public function loadXML(whichFile:String):void {						//general (reusable) function to load XML files for the program			var urlRequest:URLRequest = new URLRequest(whichFile); //loads the XML from the variable passed to the constructor			urlLoader.addEventListener(Event.COMPLETE, xmlCompleteListener, false, 0, true); //register the listener for when loading completes			urlLoader.load(urlRequest); //load the file		}				private function xmlCompleteListener(e:Event) {			//event listener function for the xml loader function			main.xmlData = new XML(urlLoader.data); //create the XML data from the url loader's data string			main.xmlLoaded = true; //set the variable to let other functions know the xml is loaded		}	}	}