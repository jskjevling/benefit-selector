package  {		import flash.display.*;	import flash.geom.*;	import flash.events.*;	import flash.net.*;	import flash.text.*;	import fl.transitions.*;	import fl.transitions.easing.*;	import flash.ui.*;	import flash.filters.*;		public class NextButton extends MovieClip {				internal var main:BenefitCalculator;		internal var bttnWidth:Number;		internal var inTransition:String;		internal var outTransition:String;		internal var theLabel:String;		internal var theVariable:String;		internal var theValue:String;		internal var nxtAction:String;		internal var whichFile:String;		internal var nxtSlide:Number;		public function TextInput() {			init();		}		private function init():void {			//do nothing		}				public function createNext(butXPos:Number, butYPos:Number, butInTrans:String, butOutTrans:String, nxtA:String, whichF:String, nxtS:Number):void {			inTransition = butInTrans;			outTransition = butOutTrans;			nxtAction = nxtA;			whichFile = whichF;			nxtSlide = nxtS;			var n:NextB = new NextB();			x = butXPos;			y = butYPos;			addChild(n); //add to this objects display list			this.addEventListener(MouseEvent.CLICK, mouseClickListener, false, 0, true);			if (inTransition=="BLINK") { //set up the properties for the BLINK transition				main.currentNext.alpha = 0;				main.currentNext.y = main.currentNext.y-50;			} else if (inTransition=="FADE") { //set up the properties of the FADE transition				main.currentNext.alpha = 0;			}			main.slideContents.addChild(main.currentNext);			main.visualManager.callNextIn(main.currentNext);		}				private function mouseClickListener(e:MouseEvent):void {			main.audioManager.stopAudio();			main.goBack.visible = false;			main.visualManager.clearScreen(nxtAction, whichFile, nxtSlide);		}	}}