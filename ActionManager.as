package  {		import flash.display.*;	import flash.geom.*;	import flash.events.*;	import flash.net.*;	import flash.text.*;	import fl.transitions.*;	import fl.transitions.easing.*;	import flash.ui.*;	import flash.filters.*;		public class ActionManager extends MovieClip {				internal var main:BenefitCalculator;		internal var theAction:String;		internal var theFile:String;		internal var toSlide:Number;		public function ActionManager() {			init();		}				private function init():void {			//do nothing		}				internal function triggerAction(whichAction:String, whichFile:String, nextSlideNumber:Number):void {			theAction = whichAction;			theFile = whichFile;			toSlide = nextSlideNumber;			switch (theAction) {				case "PLAY_SOUND":					main.audioManager.triggerAudio(theFile);					break;				case "PLAY_ANIMATION":					main.animationManager.triggerAnimation(theFile);					break;			}		}				internal function pauseAction():void {			//do nothing		}	}	}