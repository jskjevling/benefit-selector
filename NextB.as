package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class NextB extends MovieClip {		public function NextB() {			init();		}				private function init():void {			this.addEventListener(MouseEvent.MOUSE_OVER, mOver, false, 0, true);			this.addEventListener(MouseEvent.MOUSE_OUT, mOut, false, 0, true);		}				private function mOver(Event:MouseEvent):void {			this.gotoAndStop("mouseOver");		}				private function mOut(Event:MouseEvent):void {			this.gotoAndPlay("loop");		}	}	}